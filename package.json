{
    "name": "kintan_lab_chatgpt_sample_plugin",
    "version": "0.1.0",
    "scripts": {
        "pack-prod-new": "run-s pack-prod-new:pack pack-prod-new:move",
        "pack-prod-new:pack": "kintone-plugin-packer --out dist/plugin_prod.zip plugin",
        "pack-prod-new:move": "\\mv -f $(ls -1 dist/*.ppk | grep ppk | head -1) ./private_prod.ppk",
        "pack-stg-new": "run-s pack-stg-new:pack pack-stg-new:move",
        "pack-stg-new:pack": "kintone-plugin-packer --out dist/plugin_stg.zip plugin",
        "pack-stg-new:move": "\\mv -f $(ls -1 dist/*.ppk | grep ppk | head -1) ./private_stg.ppk",
        "pack-dev-new": "run-s pack-dev-new:pack pack-dev-new:move",
        "pack-dev-new:pack": "kintone-plugin-packer --out dist/plugin_dev.zip plugin",
        "pack-dev-new:move": "\\mv -f $(ls -1 dist/*.ppk | grep ppk | head -1) ./private_dev.ppk",
        "watch": "webpack --config webpack.dev.js --watch",
        "build-prod": "run-s switch-prod build-prod:webpack",
        "build-prod:webpack": "webpack --config webpack.prod.js",
        "build-stg": "run-s switch-stg build-stg:webpack",
        "build-stg:webpack": "webpack --config webpack.stg.js",
        "build-dev": "run-s switch-dev build-dev:webpack",
        "build-dev:webpack": "webpack --config webpack.dev.js",
        "switch-dev": "cp plugin/manifest.dev.json plugin/manifest.json",
        "switch-stg": "cp plugin/manifest.stg.json plugin/manifest.json",
        "switch-prod": "cp plugin/manifest.prod.json plugin/manifest.json",
        "test": "jest"
    },
    "devDependencies": {
        "@babel/core": "^7.15.0",
        "@babel/plugin-proposal-class-properties": "^7.14.5",
        "@babel/polyfill": "^7.12.1",
        "@babel/preset-env": "^7.15.0",
        "@babel/preset-typescript": "^7.15.0",
        "@cybozu/eslint-config": "^14.0.2",
        "@kintone/create-plugin": "^4.0.9",
        "@kintone/dts-gen": "^5.0.0",
        "@kintone/plugin-uploader": "^5.0.8",
        "@kintone/webpack-plugin-kintone-plugin": "^5.0.14",
        "@types/jest": "^28.1.1",
        "@types/luxon": "^2.0.9",
        "@types/node": "^16.7.1",
        "@typescript-eslint/eslint-plugin": "^4.30.0",
        "@typescript-eslint/parser": "^4.30.0",
        "babel-eslint": "^10.1.0",
        "babel-loader": "^8.2.2",
        "bootstrap": "^5.2.0",
        "css-loader": "^6.7.1",
        "eslint": "^7.32.0",
        "eslint-config-prettier": "^8.3.0",
        "eslint-plugin-prettier": "^4.0.0",
        "fork-ts-checker-webpack-plugin": "^6.3.2",
        "jest": "^28.1.1",
        "npm-run-all": "^4.1.5",
        "openai": "^3.2.1",
        "prettier": "^2.3.2",
        "sass": "^1.54.9",
        "sass-loader": "^13.0.2",
        "spin.js": "^4.1.1",
        "stats-webpack-plugin": "^0.7.0",
        "style-loader": "^3.3.1",
        "terser-webpack-plugin": "^5.3.3",
        "ts-node": "^10.2.1",
        "ts-node-dev": "^1.1.8",
        "tsc": "^2.0.4",
        "typescript": "^4.9.4",
        "webpack": "^5.74.0",
        "webpack-bundle-analyzer": "^4.6.1",
        "webpack-cli": "^4.10.0"
    },
    "dependencies": {
        "14": "^3.1.6",
        "@fortawesome/fontawesome-free": "^6.2.1",
        "@fortawesome/fontawesome-svg-core": "^6.2.1",
        "@fortawesome/react-fontawesome": "^0.2.0",
        "@kintone/kintone-ui-component": "^0.9.0",
        "@kintone/rest-api-client": "^3.1.11",
        "@shin-chan/kypes": "^0.0.1",
        "@types/bootstrap": "^5.2.0",
        "@types/micromodal": "^0.3.3",
        "fontawesome-free": "^1.0.4",
        "kintone-ui-component": "^1.8.0",
        "kintoneplugin-commonutils": "^0.0.1",
        "kintoneplugin-config-migrator": "^0.1.3",
        "luxon": "^3.2.1",
        "micromodal": "^0.4.10",
        "plugin-parameters-helper": "file:../lib/KintoneConfigBuilder.ts"
    },
    "browserslist": [
        "since 2017-06"
    ]
